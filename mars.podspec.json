{
  "public_header_files": [],
  "description": "mars",
  "license": "MIT",
  "summary": "mars",
  "requires_arc": true,
  "version": "10.2.1",
  "dependencies": {},
  "authors": {
    "gairui": "gairui@mogujie.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "homepage": "http://gitlab.mogujie.org/mwp/mars",
  "name": "mars",
  "source": {
    "git": "http://gitlab.mogujie.org/mwp/MWCS-Client.git",
    "tag": "10.2.1",
    "submodules": true
  },
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "HAVE_CONFIG_H=1 NDEBUG=1"
  },
  "source_files": [],
  "libraries": [
    "z",
    "resolv",
    "stdc++"
  ],
  "frameworks": [
    "SystemConfiguration",
    "Foundation",
    "CoreTelephony"
  ],
  "subspecs": [
    {
      "name": "comm",
      "header_mappings_dir": "mars",
      "source_files": [
        "mars/comm/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/socket/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/xlogger/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/unix/**/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/thread/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/objc/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/network/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/messagequeue/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/dns/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/coroutine/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/corepattern/*.{h,m,c,mm,cpp,cc}",
        "mars/comm/assert/*.{h,m,c,mm,cpp,cc}",
        "mars/boost/libs/**/*.{h,m,c,mm,cpp,cc}"
      ],
      "requires_arc": false,
      "private_header_files": [],
      "pod_target_xcconfig": {
        "COMM_SRC_ROOT": "$(PODS_TARGET_SRCROOT)/mars/comm",
        "HEADER_SEARCH_PATHS": "\"$(COMM_SRC_ROOT)/\" \"$(COMM_SRC_ROOT)/../\" \"$(COMM_SRC_ROOT)/../../\""
      }
    },
    {
      "name": "stn",
      "header_mappings_dir": "mars",
      "source_files": [
        "mars/stn/*.{h,m,c,mm,cpp,cc}",
        "mars/stn/src/*.{h,m,c,mm,cpp,cc}",
        "mars/stn/proto/**/*.{h}"
      ],
      "private_header_files": [],
      "pod_target_xcconfig": {
        "STN_SRC_ROOT": "$(PODS_TARGET_SRCROOT)/mars/stn",
        "HEADER_SEARCH_PATHS": "\"$(STN_SRC_ROOT)/\" \"$(STN_SRC_ROOT)/../\" \"$(STN_SRC_ROOT)/../../\" \"$(STN_SRC_ROOT)/../comm\""
      }
    },
    {
      "name": "sdt",
      "header_mappings_dir": "mars",
      "source_files": [
        "mars/sdt/*.{h,m,c,mm,cpp,cc}",
        "mars/sdt/src/**/*.{h,m,c,mm,cpp,cc}"
      ],
      "private_header_files": [],
      "pod_target_xcconfig": {
        "SDT_SRC_ROOT": "$(PODS_TARGET_SRCROOT)/mars/sdt",
        "HEADER_SEARCH_PATHS": "\"$(SDT_SRC_ROOT)/\" \"$(SDT_SRC_ROOT)/../\" \"$(SDT_SRC_ROOT)/../../\" \"$(SDT_SRC_ROOT)/src\" \"$(SDT_SRC_ROOT)/../comm\""
      }
    },
    {
      "name": "log",
      "header_mappings_dir": "mars",
      "source_files": [
        "mars/log/*.{h,m,c,mm,cpp,cc}",
        "mars/log/src/**/*.{h,m,c,mm,cpp,cc}"
      ],
      "private_header_files": [],
      "pod_target_xcconfig": {
        "LOG_SRC_ROOT": "$(PODS_TARGET_SRCROOT)/mars/sdt",
        "HEADER_SEARCH_PATHS": "\"$(LOG_SRC_ROOT)/\" \"$(LOG_SRC_ROOT)/../\" \"$(LOG_SRC_ROOT)/../../\" \"$(LOG_SRC_ROOT)/src\" \"$(LOG_SRC_ROOT)/../comm\""
      }
    },
    {
      "name": "baseevent",
      "header_mappings_dir": "mars",
      "source_files": [
        "mars/baseevent/*.{h,m,c,mm,cpp,cc}",
        "mars/baseevent/src/**/*.{h,m,c,mm,cpp,cc}"
      ],
      "private_header_files": []
    },
    {
      "name": "app",
      "header_mappings_dir": "mars",
      "source_files": [
        "mars/app/*.{h,m,c,mm,cpp,cc}"
      ],
      "private_header_files": [],
      "pod_target_xcconfig": {
        "APP_SRC_ROOT": "$(PODS_TARGET_SRCROOT)/mars/sdt",
        "HEADER_SEARCH_PATHS": "\"$(APP_SRC_ROOT)/\" \"$(APP_SRC_ROOT)/../\" \"$(APP_SRC_ROOT)/../../\" \"$(APP_SRC_ROOT)/../comm\""
      }
    }
  ],
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(inherited)\"",
    "USER_HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)\"",
    "CLANG_WARN_STRICT_PROTOTYPES": "NO",
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "NO",
    "USE_HEADERMAP": "NO",
    "ALWAYS_SEARCH_USER_PATHS": "NO"
  }
}
